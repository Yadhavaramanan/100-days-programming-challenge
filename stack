import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    Scanner scan=new Scanner(System.in);
	    Stack<Integer> st=new Stack<>();
	    int n;
	    n=scan.nextInt();
	    int arr[]=new int[n];
	    for(int i=0;i<n;i++){
	        arr[i]=scan.nextInt();
	    }
	    for(int i=0;i<n;i++){
	        st.push(arr[i]);
	    }
	    while(!st.isEmpty()){
	        System.out.print(st.pop()+" ");
	    }
	}
}
