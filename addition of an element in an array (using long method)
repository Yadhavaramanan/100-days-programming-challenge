import java.util.*;

public class Main {
    public static void main(String[] args) {
        Scanner scan = new Scanner(System.in);
        int n;
        n=scan.nextInt();
        for(int i=0;i<n;i++){
            int s=scan.nextInt();
            long sum=0;
            for(int j=0;j<s;j++){
                sum+=scan.nextLong();
            }
            
            System.out.println(sum);
        }
        
        }
}
